{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "smpkpidatafactory"
		},
		"KPISqlDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'KPISqlDB'"
		},
		"KPISqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'KPISqlServer'"
		},
		"KPISqlServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'KPISqlServer'"
		},
		"smpkpi_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'smpkpi'"
		},
		"KPISqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DanS"
		},
		"hourly_mims_archive_properties_Copy Source to Archive_parameters_FolderPath_Source": {
			"type": "string",
			"defaultValue": "mims"
		},
		"hourly_mims_archive_properties_Copy Source to Archive_parameters_LastModified_From": {
			"type": "string",
			"defaultValue": "2022-01-01"
		},
		"hourly_mims_archive_properties_Copy Source to Archive_parameters_LastModified_To": {
			"type": "string",
			"defaultValue": "2099-01-01"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "smpkpidatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"KPISqlDB_connectionString": {
						"value": "[parameters('KPISqlDB_connectionString')]"
					},
					"KPISqlServer_connectionString": {
						"value": "[parameters('KPISqlServer_connectionString')]"
					},
					"KPISqlServer_password": {
						"value": "[parameters('KPISqlServer_password')]"
					},
					"smpkpi_connectionString": {
						"value": "[parameters('smpkpi_connectionString')]"
					},
					"KPISqlServer_properties_typeProperties_userName": {
						"value": "[parameters('KPISqlServer_properties_typeProperties_userName')]"
					},
					"hourly_mims_archive_properties_Copy Source to Archive_parameters_FolderPath_Source": {
						"value": "[parameters('hourly_mims_archive_properties_Copy Source to Archive_parameters_FolderPath_Source')]"
					},
					"hourly_mims_archive_properties_Copy Source to Archive_parameters_LastModified_From": {
						"value": "[parameters('hourly_mims_archive_properties_Copy Source to Archive_parameters_LastModified_From')]"
					},
					"hourly_mims_archive_properties_Copy Source to Archive_parameters_LastModified_To": {
						"value": "[parameters('hourly_mims_archive_properties_Copy Source to Archive_parameters_LastModified_To')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}